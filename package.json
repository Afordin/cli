{
  "author": "Juan Pablo Barrera @juanpablo-is",
  "bin": {
    "afordin": "./bin/run.js"
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^4"
  },
  "description": "Afordin CLI",
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.0.0",
    "mocha": "^10",
    "oclif": "^4.0.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/juanpablo-is/cli",
  "license": "MIT",
  "main": "",
  "name": "@afordin/cli",
  "oclif": {
    "bin": "afordin",
    "dirname": "afordin",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ]
  },
  "repository": "juanpablo-is/cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "prepare": "yarn build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "version": "0.0.0",
  "bugs": "https://github.com/juanpablo-is/cli/issues",
  "keywords": [],
  "types": "dist/index.d.ts",
  "exports": "./lib/index.js",
  "type": "module"
}
